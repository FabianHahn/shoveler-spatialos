{
  "worker_type": "LauncherClient",
  "layer": "client",
  "component_delivery": {
    "default": "RELIABLE_ORDERED",
    "checkout_all_initially": true
  },
  "permissions": {
    "entity_creation": {
      "allow": false
    },
    "entity_deletion": {
      "allow": false
    },
    "entity_query": {
      "allow": true,
      "components": [
        "*"
      ]
    }
  },
  "upload_configuration": {
    "windows": {
      "localBuildDirectory": "../../build/workers/c_client/Release/assembly",
      "launcherCommand": "ShovelerCClient.exe",
      "launcherArguments": [
        "${IMPROBABLE_LOCATOR_HOSTNAME}",
        "${IMPROBABLE_PROJECT_NAME}",
        "${IMPROBABLE_DEPLOYMENT_NAME}",
        "${IMPROBABLE_LOGIN_TOKEN}"
      ]
    },
    "linux": {
      "localBuildDirectory": "../../build/workers/c_client/assembly",
      "launcherCommand": "./ShovelerCClient",
      "launcherArguments": [
        "${IMPROBABLE_LOCATOR_HOSTNAME}",
        "${IMPROBABLE_PROJECT_NAME}",
        "${IMPROBABLE_DEPLOYMENT_NAME}",
        "${IMPROBABLE_LOGIN_TOKEN}"
      ]
    }
  }
}
