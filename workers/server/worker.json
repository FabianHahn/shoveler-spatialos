{
  "worker_type": "ShovelerServer",
  "layer": "server",
  "component_delivery": {
    "default": "RELIABLE_ORDERED",
    "checkout_all_initially": true
  },
  "permissions": {
    "entity_creation": {
      "allow": true
    },
    "entity_deletion": {
      "allow": true
    },
    "entity_query": {
      "allow": true,
      "components": [
        "*"
      ]
    },
    "system_entity_command": {
      "allow": true
    }
  },
  "launch_configuration": {
    "local_deployment": {
      "windows": {
        "local_build_directory": "../../build/workers/server/Release/assembly",
        "command": "ShovelerServer.exe",
        "arguments": [
          "${IMPROBABLE_LOG_FILE}",
          "${IMPROBABLE_WORKER_ID}",
          "${IMPROBABLE_RECEPTIONIST_HOST}",
          "${IMPROBABLE_RECEPTIONIST_PORT}"
        ]
      },
      "linux": {
        "local_build_directory": "../../build/workers/server/assembly",
        "command": "ShovelerServer",
        "arguments": [
          "${IMPROBABLE_LOG_FILE}",
          "${IMPROBABLE_WORKER_ID}",
          "${IMPROBABLE_RECEPTIONIST_HOST}",
          "${IMPROBABLE_RECEPTIONIST_PORT}"
        ]
      }
    },
    "cloud_deployment": {
      "linux": {
        "local_build_directory": "../../build/workers/server/assembly",
        "command": "ShovelerServer",
        "arguments": [
          "${IMPROBABLE_LOG_FILE}",
          "${IMPROBABLE_WORKER_ID}",
          "${IMPROBABLE_RECEPTIONIST_HOST}",
          "${IMPROBABLE_RECEPTIONIST_PORT}"
        ]
      }
    }
  }
}
